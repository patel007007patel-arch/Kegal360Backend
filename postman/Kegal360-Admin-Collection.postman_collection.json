{
  "info": {
    "name": "Kegal360 Admin API",
    "description": "Complete admin-only endpoints. Use an admin user token in Authorization header.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    { "key": "baseUrl", "value": "http://localhost:5000" },
    { "key": "token", "value": "" }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [{ "key": "token", "value": "{{token}}", "type": "string" }]
  },
  "item": [
    {
      "name": "Dashboard",
      "item": [
        {
          "name": "Get Dashboard Stats",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{baseUrl}}/api/admin/dashboard/stats"
          }
        }
      ]
    },
    {
      "name": "Profile & Settings",
      "item": [
        {
          "name": "Get Admin Profile",
          "request": {
            "method": "GET",
            "header": [],
            "url": "{{baseUrl}}/api/admin/profile",
            "description": "Get current admin's profile information (email, name, role)."
          }
        },
        {
          "name": "Update Admin Profile",
          "request": {
            "method": "PUT",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Admin Name\",\n  \"email\": \"admin@example.com\"\n}"
            },
            "url": "{{baseUrl}}/api/admin/profile",
            "description": "Update admin profile (name, email). **All fields optional.** Email must be unique."
          }
        },
        {
          "name": "Change Password",
          "request": {
            "method": "PUT",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"currentPassword\": \"oldpassword123\",\n  \"newPassword\": \"newpassword123\"\n}"
            },
            "url": "{{baseUrl}}/api/admin/profile/change-password",
            "description": "Change admin password. **Required:** currentPassword, newPassword (min 6 chars). Use this in Profile/Settings page when logged in."
          }
        },
        {
          "name": "Forgot Password (Request OTP)",
          "request": {
            "auth": { "type": "noauth" },
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@example.com\"\n}"
            },
            "url": "{{baseUrl}}/api/auth/forgot-password",
            "description": "Request OTP for password reset. **Required:** email. Works for admin users too. In development, add ?dev=1 to see OTP in response."
          }
        },
        {
          "name": "Reset Password (With OTP)",
          "request": {
            "auth": { "type": "noauth" },
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@example.com\",\n  \"otp\": \"123456\",\n  \"newPassword\": \"newpassword123\"\n}"
            },
            "url": "{{baseUrl}}/api/auth/reset-password",
            "description": "Reset password using OTP from forgot-password. **Required:** email, otp (6 digits), newPassword (min 6 chars). Use this on login page when admin forgot password."
          }
        }
      ]
    },
